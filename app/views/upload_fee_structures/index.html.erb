<div class="row">
  <div class="col-sm-10">
    <h1>Upload Fee Structures</h1> 
  </div>
  <% if current_user.is_admin? %>
    <div class="newhead col-sm-2">
      <h1>
      <%= link_to 'New', new_upload_fee_structure_path, class: "btn btn-default btn-md" %>
      </h1>
    </div>
  <% end %>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Upload</th>
      <th>Status</th>
      <th>Number of success</th>
      <th>Number of fail</th>
      <th>Total record</th>
      <th>Fail error</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_fee_structures.each do |upload_fee_structure| %>
      <tr>
        <td><%= upload_fee_structure.upload_file_name %></td>
        <td><%= UPLOAD_STATUS[upload_fee_structure.status] %></td>
        <td><%= upload_fee_structure.number_of_success %></td>
        <td><%= upload_fee_structure.number_of_fail %></td>
        <td><%= upload_fee_structure.total_record %></td>
        <td><%= upload_fee_structure.fail_error %></td>
        <td><%= link_to 'Processed', upload_fee_structure.get_processed_file_name, class: "btn btn-xs btn-primary", type:'button' %></td>
        <td><%= link_to 'Destroy', upload_fee_structure, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-xs btn-danger"%></td>
      </tr>
    <% end %>
  </tbody>
</table>
